{
    "source": {
        "include": ["package/dist.js"],
        "includePattern": ".js$"
    },

    "plugins": ["plugins/markdown"],

    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "theme": "dark"
        }
    }
}